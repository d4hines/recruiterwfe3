{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-airbnb"],
  "defaultSeverity": "warning",
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "member-access": false,
    "no-boolean-literal-compare": false
  }
}